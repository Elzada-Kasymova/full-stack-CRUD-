datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  id      Int     @id @default(autoincrement())
  name    String
  email   String  @unique
  job     String?
  rate    Float   @default(100.00)
  isActive Boolean @default(true)
}
